<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal">

    <!-- <TextView
         android:id="@+id/logistics_time"
         android:layout_width="@dimen/bj_60dp"
         android:layout_height="@dimen/bj_75dp"
         android:layout_marginLeft="@dimen/bj_26dp"
         android:gravity="center"
         android:text="2019-12-12  06  :06"
         android:textColor="@color/black_000000"
         android:textSize="@dimen/wz_11sp" />-->

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/bj_75dp"
        android:gravity="center"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="@dimen/bj_42dp"
            android:layout_height="match_parent"
            android:gravity="center"
            android:orientation="vertical">

            <View
                android:id="@+id/line_top"
                android:layout_width="@dimen/bj_1dp"
                android:layout_height="@dimen/bj_35dp"
                android:background="@color/black_000000" />

            <ImageView
                android:id="@+id/cycle"
                android:layout_width="@dimen/bj_10dp"
                android:layout_height="@dimen/bj_10dp"
                android:layout_gravity="center"
                android:src="@drawable/shape_round" />

            <View
                android:id="@+id/line_botton"
                android:layout_width="@dimen/bj_1dp"
                android:layout_height="@dimen/bj_35dp"
                android:background="@color/black_000000" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/bj_10dp"
            android:orientation="vertical">

            <TextView
                android:id="@+id/logistics_name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:maxLines="2"
                android:textColor="@color/green_1baf4f"
                android:textSize="@dimen/wz_12sp" />

            <TextView
                android:id="@+id/logistics_time"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="@dimen/bj_11dp"
                android:singleLine="true"
                android:textSize="@dimen/wz_10sp" />

        </LinearLayout>

    </LinearLayout>
</LinearLayout>